{
  "name": "fmhero-web-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tailwind": "npx tailwindcss -i resources/sass/app.scss -o resources/sass/staging.scss --watch --jit --purge='src/**/*.ts' --purge='src/**/*.tsx'",
    "test": "ts-mocha --project tsconfig.json --paths test/**/*.spec.ts",
    "serve:admin": "cross-env NODE_ENV=development APP_DOMAIN=admin DEV_MODE=serve node_modules/webpack/bin/webpack.js serve --progress --config webpack.dev.js",
    "serve:user": "cross-env NODE_ENV=development APP_DOMAIN=user DEV_MODE=serve node_modules/webpack/bin/webpack.js serve --progress --config webpack.dev.js",
    "build:admin": "cross-env NODE_ENV=production APP_DOMAIN=admin node_modules/webpack/bin/webpack.js --config webpack.prod.js",
    "build:user": "cross-env NODE_ENV=production APP_DOMAIN=user node_modules/webpack/bin/webpack.js --config webpack.prod.js",
    "watch:admin": "cross-env NODE_ENV=development APP_DOMAIN=admin DEV_MODE=watch node_modules/webpack/bin/webpack.js watch --progress --config webpack.dev.js",
    "watch:user": "cross-env NODE_ENV=development APP_DOMAIN=user DEV_MODE=watch node_modules/webpack/bin/webpack.js watch --progress --config webpack.dev.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "chart.js": "^3.3.2",
    "luxon": "^1.26.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@types/luxon": "^1.25.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^10.2.6",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "dotenv": "^10.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.2.0",
    "mini-css-extract-plugin": "^1.3.8",
    "mocha": "^9.0.0",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^5.0.0",
    "postcss-purgecss": "^2.0.3",
    "postcss-scss": "^3.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "tailwindcss": "^2.2.2",
    "ts-loader": "^8.0.17",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.1.5",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
